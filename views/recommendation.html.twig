    <!-- TAGS BAR -->
    <div class="tagsBarContainer animated fadeInDown">
        <div id="tagsBarInside" class="tagsBarInside ">
            <div class="tagsBarItem tagsBarItemEmpty">
                <p class="tagsBarItemP"> Choisissez votre premier critère </p>
            </div>
