            </div>
        </div>
        <div class="tagsBarFade">
        </div>
        <div class="tagsBarNext tagsBarNextDisable">

            <form action="{{ path('recommendationResults') }}" method="POST">

                <input class="recoPost" type="hidden" name="artvivre" value="notchecked">
                <input class="recoPost" type="hidden" name="solidarite" value="notchecked">
