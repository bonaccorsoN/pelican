                <input id="eclatPost" type="hidden" name="score_eclate" value="0">
                <input id="investPost" type="hidden" name="score_investir" value="0">
                <input id="culturePost" type="hidden" name="score_culture" value="0">
                <input id="humaPost" type="hidden" name="score_humanitaire" value="0">

                <input id="submitUserScore" type="submit" class="form-control btn btn-default" value="5 Restants" name="submitrecommendation">

